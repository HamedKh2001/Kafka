{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ProducerConfig": {
    "MaxRange": "40000",
    "CountPerStep": "200"
  },
  "KafkaConfigs": {
    "Bootstrapserver": "localhost:9092"
  },
  "QuartzConfig": {
    "CronExpression": "0 * * ? * *"
  }
}