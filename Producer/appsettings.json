{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ProducerConfig": {
    "MaxRange": "100000", //Copacity Of Dictionary
    "OutputRate": "1000", //Send Messages Per Second
    "ReportInterval": "1000" //Per milisecond
  },
  "KafkaConfigs": {
    "Bootstrapserver": "localhost:9092"
  },
  "QuartzConfig": {
    "CronExpression": "0 * * ? * *"
  }
}